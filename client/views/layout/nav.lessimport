nav {
  text-align:right;
  display: block;

  ul {
    display:inline;
    margin-left:0;
    padding-left:0;

    li {
      display:inline;
      margin-left:15px;
      padding-left:0px;
      font-weight: bold;
    }
    a {
      color: #000;
      text-decoration: none;
      font-weight: bold;
    }
    a:visited {
      color: #000;
      text-decoration: none;
    }
  }
}
