.carousel {
  #effects > .raised;
  .clearfix;
  max-width: 1024px;
  margin: auto;
  margin-bottom: 15px;

  .spin-container {
    #spinner {
      .make-sm-column(12);
      top: 50%;
    }
  }

  .item {
    img {
      width: 100%;
      height: 100%;
    }
    .carousel-caption {
      color: #FFF;
      background: none;
    }
  }
}

